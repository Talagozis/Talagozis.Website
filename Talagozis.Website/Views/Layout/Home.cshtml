@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet

@model Talagozis.Website.Models.ViewModels.ViewModel

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>Talagozis Christos CV</title>

	<link rel="shortcut icon" href="favicon.ico?v=2" type="image/x-icon">
	<link rel="icon" href="favicon.ico?v=2" type="image/x-icon">

	<meta name="google-site-verification" content="S1X7cY9BACeSAkVE-OOhvwmyAYzRrhFSO7OF1Lx0jvg" />

    <environment names="Development">
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Open+Sans:400,300,700" />
        @*<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Montserrat:400,500,600,700,800" />*@
        <link rel="stylesheet" type="text/css" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
        <link rel="stylesheet" type="text/css" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">

        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/main.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/default-color.manisa.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/manisa-icon.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/lightbox.min.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/slick.css" />

        <link rel="stylesheet" type="text/css" href="~/css/home/custom.css?v=20181213" />

        @RenderSection("css", true)
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Open+Sans:400,300,700" />
        @*<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Montserrat:400,500,600,700,800" />*@
        <link rel="stylesheet" type="text/css" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
        <link rel="stylesheet" type="text/css" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">

        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/main.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/default-color.manisa.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/manisa-icon.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/lightbox.min.css" />
        <link rel="stylesheet" type="text/css" href="~/lib/manisa/css/slick.css" />

        <link rel="stylesheet" type="text/css" href="~/css/home/custom.css?v=20181213" />

        @RenderSection("css", true)
    </environment>

	@Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body style="overflow: auto;">

    <nav class="navbar navbar-inverse navbar-expand-lg @(Model.htmlHeader?.breadcrumps != null && Model.htmlHeader.breadcrumps.Any() ? "smooth" : string.Empty)">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index" style="font-size: 35px; letter-spacing: 1px; font-family: Bebas Neue; line-height: 30px;">
                    <span style="color: #3ac3cb;">CHRISTOS</span><span class="grey" style="color: #424242 ;">TALAGOZIS</span>
                </a>
            </div>

            <div class="collapse navbar-collapse">
                <ul class="navbar-nav mr-auto"></ul>
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a asp-area="" asp-controller="Home" asp-action="Blog">Blog</a>
                    </li>
                    <li class="nav-item">
                        <a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
                    </li>
                    <li class="nav-item strong">
                        <a asp-area="" asp-controller="Home" asp-action="CV">CV</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    @if (Model.htmlHeader?.breadcrumps != null && Model.htmlHeader.breadcrumps.Any())
    {
        <nav class="breadcrumb" aria-label="breadcrumb">
            <div class="container">
                <ol class="breadcrumb">
                    @foreach (var breadcrump in Model.htmlHeader.breadcrumps)
                    {
                        <li class="breadcrumb-item">
                            @if (!string.IsNullOrWhiteSpace(breadcrump.page))
                            {
                                <a asp-page="@breadcrump.page">@breadcrump.text</a>
                            }
                            else if (!string.IsNullOrWhiteSpace(breadcrump.plainSlug))
                            {
                                <a href="@breadcrump.plainSlug">@breadcrump.text</a>
                            }
                            else
                            {
                                <a asp-area="@breadcrump.area" asp-controller="@breadcrump.controller" asp-action="@breadcrump.action">@breadcrump.text</a>
                            }
                        </li>
                    }
                </ol>
            </div>
        </nav>
    }

    <div class="container-fluid">
        @RenderBody()
    </div>

    <footer class="text-center" style="border-top: solid #e2e2e2 1px;">    
        @{ Html.RenderPartial("~/Views/Home/_footer.cshtml"); }
        @{ Html.RenderPartial("~/Views/Layout/Global/subfooter.cshtml"); }
    </footer>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <!-- <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script> -->
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

        <script src="~/lib/vertica/js/main.js" asp-append-version="true"></script>
        <script src="~/lib/vertica/vendor/jquery.easing.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.mousewheel.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.nicescroll.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.nicescroll.plus.js"></script>
        <script src="~/lib/vertica/vendor/waypoints.min.js"></script>
        <script src="~/lib/vertica/vendor/nivo-lightbox/nivo-lightbox.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.bxslider.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.validate.min.js"></script>
        <script src="~/lib/vertica/vendor/placeholders.jquery.min.js"></script>
        <script src="~/lib/vertica/vendor/cross-browser.js"></script>

        @RenderSection("js", true)
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.2.1.min.js" asp-fallback-src="~/lib/jquery/dist/jquery.min.js" asp-fallback-test="window.jQuery" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

        <script src="~/lib/vertica/js/main.js" asp-append-version="true"></script>
        <script src="~/lib/vertica/vendor/jquery.easing.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.mousewheel.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.nicescroll.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.nicescroll.plus.js"></script>
        <script src="~/lib/vertica/vendor/waypoints.min.js"></script>
        <script src="~/lib/vertica/vendor/nivo-lightbox/nivo-lightbox.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.bxslider.min.js"></script>
        <script src="~/lib/vertica/vendor/jquery.validate.min.js"></script>
        <script src="~/lib/vertica/vendor/placeholders.jquery.min.js"></script>
        <script src="~/lib/vertica/vendor/cross-browser.js"></script>

        @RenderSection("js", true)
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
