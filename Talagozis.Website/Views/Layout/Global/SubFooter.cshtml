<div class="container-fluid d-none d-sm-block">
    <div class="row" style="background: #21241c; padding: 3px 0; height:26px; margin-bottom: 0;">
        <div class="col-xs-3 col-sm-3">
            <p style="margin: 1px 0; text-align: left; color: #3D3D3D; font-size: 11px;">v@(ViewBag.AssemblyVersion)</p>
        </div>
        <div class="col-xs-6 col-sm-6">
            <p style="margin: 1px 0; text-align: center; color: white; font-size: 13px;">Powered by <a style="color:#00bcd4">@ViewBag.AssemblyTitle</a></p>
        </div>
        <div class="col-xs-3 col-sm-3">
            <p style="margin: 1px 0; text-align: right; color: #3D3D3D; font-size: 11px;">@((DateTime.Now - (ViewBag.startTime as DateTime?).GetValueOrDefault()).TotalSeconds.ToString("N3"))s</p>
        </div>
    </div>
</div>